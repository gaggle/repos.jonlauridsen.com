{
  "name": "repos.jonlauridsen.com",
  "scripts": {
    "clean": "rm -rf tmp/",
    "deploy:gh-pages": "./scripts/deploy tmp/out/ gh-pages",
    "deploy:gh-scraped": "./scripts/deploy tmp/scraped/ gh-scraped",
    "export": "repo-web http://list.jonlauridsen.com -s=tmp/scraped/ -o=tmp/out/ --production",
    "fetch:gh-scraped": "./scripts/fetch gh-scraped tmp/scraped/",
    "postexport": "touch tmp/out/.nojekyll && echo repos.jonlauridsen.com > tmp/out/CNAME",
    "scrape": "repo-scrape github -o=tmp/scraped/",
    "test": "echo No tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaggle/repos.jonlauridsen.com.git"
  },
  "author": "Jon Lauridsen <mail@jonlauridsen.com>",
  "license": "ISC",
  "dependencies": {
    "repo-lister": "git+https://github.com/gaggle/repo-lister.git",
    "repo-scraper": "git+https://github.com/gaggle/repo-scraper.git"
  }
}
